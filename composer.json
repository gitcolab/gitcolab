{
    "name": "gitcolab/gitcolab",
    "license": "MIT",
    "type": "project",
    "description": "Git repositories inside your infrastructure",
    "autoload": {
        "psr-4": { "": "src/" },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "~2.8",
        "doctrine/orm": "~2.5",
        "doctrine/doctrine-bundle": "~1.6",
        "doctrine/doctrine-migrations-bundle": "1.1",
        "twig/extensions": "~1.3",
        "symfony/assetic-bundle": "~2.7",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.9",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "~2.1",
        "stof/doctrine-extensions-bundle": "~1.2",
        "knplabs/knp-menu-bundle": "~2.1",
        "jms/serializer-bundle": "~1.1",
        "friendsofsymfony/rest-bundle": "~1.7",
        "friendsofsymfony/jsrouting-bundle": "~1.6",
        "willdurand/js-translation-bundle": "~2.5",
        "white-october/pagerfanta-bundle": "~1.0",
        "gitonomy/gitlib": "~0.1",
        "gitonomy/git-bundle": "0.2.*",
        "michelf/php-markdown": "~1.6",
        "tempo/resource-extra-bundle":"0.6.x-dev"
    },
    "require-dev": {
        "behat/behat": "~3.1",
        "behat/mink-extension": "~2.2",
        "behat/mink-browserkit-driver": "~1.3",
        "behat/mink-selenium2-driver": "~1.3",
        "behat/symfony2-extension": "~2.1",
        "behat/mink-goutte-driver": "~1.2",
        "knplabs/friendly-contexts": "dev-master",
        "doctrine/doctrine-fixtures-bundle": "~2.3",
        "doctrine/data-fixtures": "~1.1"
    },
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "plateform": {
            "php": ">=5.5.9"
        }
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
